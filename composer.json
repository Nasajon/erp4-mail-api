{
    "name": "nasajon/erp4-mail-api",
    "license": "MIT",
    "type": "project",
    "description": "Aplicação responsável por envio de emails.",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": ["app/AppKernel.php", "app/AppCache.php"]
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "^3.3",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "symfony/monolog-bundle": "^3.1.0",
        "sensio/framework-extra-bundle": "^3.0.2",      
        "jms/serializer-bundle": "^2.3",
        "friendsofsymfony/rest-bundle": "^2.3",  
        "friendsofsymfony/jsrouting-bundle": "^1.5",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "firebase/php-jwt": "2.*",
        "exercise/htmlpurifier-bundle": "^0.2.2",
        "nasajon/SDK": "dev-master",
        "nasajon/login-bundle": "dev-master",
        "sentry/sentry-symfony": "^2.0",
        "nasajon/componentes-interface-bundle": "dev-master",
        "lightsaml/sp-bundle": "^1.1",
        "knplabs/knp-gaufrette-bundle": "~0.4",
        "aws/aws-sdk-php-symfony": "~1.0",
        "guzzlehttp/guzzle": "^6.0",
        "bernard/bernard": "^1.0.0-alpha9",
        "bernard/bernard-bundle": "^2.0",
        "php-amqplib/rabbitmq-bundle": "^1.14.1",
        "swiftmailer/swiftmailer": "6.0",
        "defuse/php-encryption": "^2.3"
    },
    "require-dev": {
        "sensio/distribution-bundle": "^5.0.19",
        "nasajon/mdatransformer": "dev-master",
        "codeception/codeception": "^2.3.7",
        "doctrine/doctrine-migrations-bundle": "^1.3"
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "process-timeout": 3600
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "app",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:Nasajon/LoginBundle"
        },
        {
            "type": "git",
            "url": "git@github.com:Nasajon/SDK"
        },
        {
            "type": "git",
            "url": "git@github.com:Nasajon/ComponentesInterfaceBundle"
        },
        {
            "type": "git",
            "url": "git@github.com:Nasajon/MdaPhpTransformer"
        }
    ]
}
